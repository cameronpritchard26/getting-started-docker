<div id="message">
  {{ message }}
</div>
<div id="info">
  <table>
    <tr>
      <th>Pod/container/host that serviced this request:</th>
      <td>{{ hostName }}</td>
    </tr>
    <tr>
      <th>Version from app.js:</th>
      <td>{{ version }} - {{ currentTimestamp }}</td>
    </tr>
  </table>

</div>